{
  "name": "crater-invoice/crater",
  "description": "Free & Open Source Invoice App for Individuals & Small Businesses. https://craterapp.com",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^8.1",
    "aws/aws-sdk-php": "^3.142",
    "barryvdh/laravel-dompdf": "^2.0",
    "crater-invoice/modules": "^1.0.0",
    "doctrine/dbal": "^3.0",
    "dragonmantank/cron-expression": "^3.1",
    "guzzlehttp/guzzle": "^7.2",
    "intervention/image": "^2.3",
    "jasonmccreary/laravel-test-assertions": "^2.3",
    "laravel/framework": "^10.8",
    "laravel/helpers": "^1.6",
    "laravel/sanctum": "^3.2",
    "laravel/tinker": "^2.8",
    "laravel/ui": "^4.0",
    "lavary/laravel-menu": "^1.8",
    "league/flysystem-aws-s3-v3": "^3.0",
    "predis/predis": "^2.1",
    "silber/bouncer": "v1.0.1",
    "spatie/flysystem-dropbox": "^3.0",
    "spatie/laravel-backup": "^8.0",
    "spatie/laravel-medialibrary": "^10.0",
    "vinkla/hashids": "^11.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "friendsofphp/php-cs-fixer": "^3.0",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^7.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-faker": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0",
    "phpunit/phpunit": "^10.1",
    "spatie/laravel-ignition": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Crater\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "Modules\\": "Modules/"
    },
    "files": [
      "app/Space/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  }
}
